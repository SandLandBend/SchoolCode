  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->RGBColor[1, 1, 0]],Cell["\<\PlaneBar2Stiffness[ncoor_,Em_,A_,options_]:= Module[ {x1,x2,y1,y2,x21,y21,EA,numer,L,LL,LLL,Ke},   {{x1,y1},{x2,y2}}=ncoor; {x21,y21}={x2-x1,y2-y1};  EA=Em*A; {numer}=options; LL=x21^2+y21^2; L=Sqrt[LL];  If [numer,{x21,y21,EA,LL,L}=N[{x21,y21,EA,LL,L}]];   If [!numer, L=PowerExpand[L]]; LLL=Simplify[LL*L];  Ke=(Em*A/LLL)*{{ x21*x21, x21*y21,-x21*x21,-x21*y21},                 { y21*x21, y21*y21,-y21*x21,-y21*y21},                 {-x21*x21,-x21*y21, x21*x21, x21*y21},                 {-y21*x21,-y21*y21, y21*x21, y21*y21}};   Return[Ke]];   \\>", "Input",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->GrayLevel[0.899992]],Cell["\<\The following script evaluates Ke and  perform various tests on a  \2-node bar element numerically defined as going from  (0,0) to (30,40), with Em=1000, and  \A=5.\\>", "Text",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->RGBColor[1, 1, 0]],Cell["\<\ClearAll[A,Em,L];ncoor={{0,0},{30,40}}; Em=1000; A=5; Ke= PlaneBar2Stiffness[ncoor,Em,A,{True}];Print[\"Numerical Elem Stiff Matrix: \"];Print[Ke//MatrixForm];Print[\"Eigenvalues of Ke=\",Chop[Eigenvalues[N[Ke]]]];Print[\"Symmetry check=\",Simplify[Chop[Transpose[Ke]-Ke]]];\\>", "Input",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->GrayLevel[0.899992]],Cell["\<\The following script evaluates Ke symbolically for a bar of length \L,with properties  Em and A kept in symbolic form, and performs various tests.\\>", \"Text",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->RGBColor[1, 1, 0]],Cell["\<\ClearAll[A,Em,L];ncoor={{0,0},{L,0}};Ke=  PlaneBar2Stiffness[ncoor,Em,A,{False}]; kfac=Em*A/L; Ke=Simplify[Ke/kfac];Print[\"Symbolic Elem Stiff Matrix: \"]; Print[kfac,\" \",Ke//MatrixForm];Print[\"Eigenvalues of Ke=\",kfac,\"*\",Eigenvalues[Ke]];\\>", "Input",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->GrayLevel[0.899992]],Cell["\<\The following module computes the linear element stiffness matrix \Ke of a three-dimensional, two-node, prismatic bar element.\\>", "Text",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->RGBColor[1, 1, 0]],Cell["\<\SpaceBar2Stiffness[ncoor_,Em_,A_,options_]:=Module[ {x1,x2,y1,y2,z1,z2,x21,y21,z21,EA,numer,L,LL,LLL,Ke},  {{x1,y1,z1},{x2,y2,z2}}=ncoor; {x21,y21,z21}={x2-x1,y2-y1,z2-z1};  EA=Em*A; {numer}=options;  LL=x21^2+y21^2+z21^2; L=Sqrt[LL];  If [numer,{x21,y21,z21,EA,LL,L}=N[{x21,y21,z21,EA,LL,L}]];  If [!numer, L=PowerExpand[L]]; LLL=Simplify[LL*L];  Ke=(Em*A/LLL)*     {{ x21*x21, x21*y21, x21*z21,-x21*x21,-x21*y21,-x21*z21},      { y21*x21, y21*y21, y21*z21,-y21*x21,-y21*y21,-y21*z21},      { z21*x21, z21*y21, z21*z21,-z21*x21,-z21*y21,-z21*z21},      {-x21*x21,-x21*y21,-x21*z21, x21*x21, x21*y21, x21*z21},      {-y21*x21,-y21*y21,-y21*z21, y21*x21, y21*y21, y21*z21},      {-z21*x21,-z21*y21,-z21*z21, z21*x21, z21*y21, z21*z21}};   Return[Ke];]; \\>", "Input",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->GrayLevel[0.899992]],Cell["\<\The following script evaluates Ke and  perform various tests on a  \2-node  3D bar element numerically defined as going from  (0,0) to (2,3,6) ie length 7,  with Em=343 \and  A=10 \\>", "Text",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->RGBColor[1, 1, 0]],Cell["\<\ClearAll[A,Em];ncoor={{0,0,0},{2,3,6}}; Em=343; A=10;Ke= SpaceBar2Stiffness[ncoor,Em,A,{True}];Print[\"Numerical Elem Stiff Matrix: \"];Print[Ke//MatrixForm];Print[\"Eigenvalues of Ke=\",Chop[Eigenvalues[Ke]]];\\>", "Input",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->GrayLevel[0.899992]],Cell["\<\The following script evaluates Ke symbolically for a 3D bar of \length L,with properties Em and A kept in symbolic form, and performs various tests.\\>", \"Text",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->RGBColor[1, 1, 0]],Cell[CellGroupData[{Cell["\<\ClearAll[A,Em,L];ncoor={{0,0,0},{L,2*L,2*L}/3}; Ke=  SpaceBar2Stiffness[ncoor,Em,A,{False}];kfac=Em*A/(9*L); Ke=Simplify[Ke/kfac];Print[\"Symbolic Elem Stiff Matrix: \"]; Print[kfac,\" \",Ke//MatrixForm];Print[\"Eigenvalues of Ke=\",kfac,\"*\",Eigenvalues[Ke]];\\>", "Input",  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->GrayLevel[0.899992]],Cell["\<\n=12;For [i=1,i<=n,i++,For [j=1,j<=i,j++,For [k=1,k<=j,k++,For [m=1,m<=3*n,m++,If [m^2==i^2+j^2+k^2,Print[{i,j,k,m}]]]]]];\\>", "Input",\  CellFrame->True,  CellMargins->{{12, 78}, {Inherited, Inherited}},  InitializationCell->True,  CellLabelMargins->{{6, Inherited}, {Inherited, Inherited}},  ImageRegion->{{-0, 1}, {0, 1}},  Background->RGBColor[0, 1, 0]]}, Open  ]]},FrontEndVersion->"4.2 for Macintosh",ScreenRectangle->{{0, 1920}, {0, 1180}},AutoGeneratedPackage->None,WindowToolbars->{},CellGrouping->Manual,WindowSize->{928, 1014},WindowMargins->{{Automatic, 262}, {Automatic, 2}},PrivateNotebookOptions->{"ColorPalette"->{RGBColor, -1}},ShowCellLabel->True,ShowCellTags->False,RenderingOptions->{"ObjectDithering"->True,"RasterDithering"->False},MacintoshSystemPageSetup->"\<\00<0001804P000000]P2:?oQon82n@960dL5:0?l0080001804P000000]P2:0010000I00000400`<300000BL?00400@00000000000000060801T1T0000000000000000000000000000000000000000000\>"](*******************************************************************Cached data follows.  If you edit this Notebook file directly, notusing Mathematica, you must remove the line containing CacheID atthe top of  the file.  The cache data will then be recreated whenyou save this file from within Mathematica.*******************************************************************)(*CellTagsOutlineCellTagsIndex->{}*)(*CellTagsIndexCellTagsIndex->{}*)(*NotebookFileOutlineNotebook[{Cell[1754, 51, 374, 10, 62, "Text",  InitializationCell->True],Cell[2131, 63, 812, 19, 208, "Input",  InitializationCell->True],Cell[2946, 84, 418, 11, 62, "Text",  InitializationCell->True],Cell[3367, 97, 534, 14, 133, "Input",  InitializationCell->True],Cell[3904, 113, 401, 11, 62, "Text",  InitializationCell->True],Cell[4308, 126, 507, 14, 133, "Input",  InitializationCell->True],Cell[4818, 142, 379, 10, 62, "Text"